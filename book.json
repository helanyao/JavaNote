{
    "title": "Personal Knowledge Notes of Java",
    "author": "Jinghua ZHU",

    "plugins": [
        "image-captions",
        "mathjax", 
        "anchors",
        "splitter",
        "github",
        "github-buttons",
        "prism", 
        "-highlight",
        "book-summary-scroll-position-saver",
        "tbfed-pagefooter",
        "expandable-chapters",
        "search"
    ],

    "pluginsConfig": {
        "github-buttons": {
            "repo": "jinghzhu/JavaNote",
            "types": [
                "star",
                "watch"
            ],
            "size": "small"
        },
        "github": {
            "url": "https://github.com/jinghzhu/JavaNote"
        },
        "mathjax":{
            "forceSVG": false
        },
        "tbfed-pagefooter": {
            "Contact":"jhzhu@outlook.com",
            "modify_label": "Last Modified:",
            "modify_format": "DD-MM-YYYY"
        }
    },

    "pdf": {
        "toc": true,
        "pageNumbers": false,
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        }
    }
}